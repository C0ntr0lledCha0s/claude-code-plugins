{
  "enabledPlugins": {
    "agent-builder@claude-code-plugin-automations": true,
    "self-improvement@claude-code-plugin-automations": true,
    "github-workflows@claude-code-plugin-automations": true
  },
  "extraKnownMarketplaces": {
    "claude-code-plugin-automations": {
      "source": {
        "source": "directory",
        "path": "./"
      }
    }
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$CLAUDE_PROJECT_DIR/.claude/hooks/session-start.sh"
          }
        ]
      }
    ]
  }
}
